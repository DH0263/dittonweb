{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile",
    "watchPatterns": ["**"]
  },
  "deploy": {
    "startCommand": "python main.py",
    "healthcheckPath": "/",
    "restartPolicyType": "ALWAYS",
    "restartPolicyMaxRetries": 10
  }
}
